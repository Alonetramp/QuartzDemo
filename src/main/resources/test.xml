<?xml version="1.0" encoding="UTF-8"?>
<QuartzConfig>
    <Schedules>
        <Schedule c1="0">
            <JobDetail name = "myJob" group="Job" Jobclazz="com.example.Quartz.HelloWorldJob"/>
            <Trigger name="myJob" group="Job">
                <description>定时器</description>
                <startTime>2010-01-01 09:00:00</startTime>
                <corn-expression>*/1 * * * * ?</corn-expression>
            </Trigger>
        </Schedule>
    </Schedules>
</QuartzConfig>